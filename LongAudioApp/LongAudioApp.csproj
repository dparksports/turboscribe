<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <UseWPF>true</UseWPF>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <ApplicationIcon></ApplicationIcon>
    <AssemblyName>TurboScribe</AssemblyName>
    <RootNamespace>LongAudioApp</RootNamespace>
    <Version>3.3.2</Version>
  </PropertyGroup>
  <ItemGroup>
    <None Include="..\fast_engine.py" CopyToOutputDirectory="PreserveNewest" Link="fast_engine.py" />
    <None Include="..\timestamp_engine.py" CopyToOutputDirectory="PreserveNewest" Link="timestamp_engine.py" />
    <Content Include="..\firebase_config.json" CopyToOutputDirectory="PreserveNewest" Link="firebase_config.json" />

  </ItemGroup>
</Project>
